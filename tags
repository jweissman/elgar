Sheet	lib/elgar.rb	/^  class Sheet < Struct.new(:name)$/;"	c	class:Elgar
read	lib/elgar.rb	/^    def read(address)$/;"	f	class:Elgar.Sheet
write	lib/elgar.rb	/^    def write(data, at:)$/;"	f	class:Elgar.Sheet
